@{
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
@section Container{
    <style>
        .search-box__cont{display:unset; text-align:center;}
        .search-box__cont > div:not(:last-child){margin-bottom:15px;}
        .search-box__input.search{width:100%!important;}
    </style>
    <script src="~/Scripts/Ocean/order.js"></script>
    <script src="https://code.responsivevoice.org/responsivevoice.js?key=vTAXU6E5"></script>
    <section class="container">
        <div class="main_contents">
            <div class="search-box">
                <div style="display:flex; align-items:center; gap:10px; position:absolute; right:12px;">
